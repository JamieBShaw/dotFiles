`
  //Prettier
  "prettier.eslintIntegration": true,
  "prettier.tslintIntegration": false,
  "prettier.singleQuote": true,
  //Snippets
  "snippets.userSnippetsDirectory": "~/.config/nvim/snips",
  "snippets.extends": {
    "typescript": ["typescriptreact"],
    "typescriptreact": ["typescript"]
  },
  // COC
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "html",
    "css",
    "typescriptreact",
    "javascript",
    "javascriptreact",
    "graphql",
    "json",
    "java",
    "yaml",
    "yml",
    "go",
    "python"
  ],
  // TSServer
  "tsserver.npm": "/usr/bin/npm",
  "tsserver.enableJavascript": true,
  "tsserver.log": "verbose",
  // Typescript
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enable": true,
  // Javascript
  "javascript.suggest.autoImports": true,
  "javascript.validate.enable": true,
  "javascript.format.enabled": true,
  "javascript.format.semicolons": "insert",
  "javascript.updateImportsOnFileMove.enable": true,
  "suggest.noselect": false,
  // Python
  "python.linting.enabled": true,
  "python.pythonPath": "/usr/bin/python3.8",
  "python.jediEnabled": false,
  "python.formatting.provider": "black",
  "python.autoUpdateLanguageServer": true,
  "python.analysis.diagnosticEnabled": true,
  "languageserver": {
    "golang": {
      "command": "/home/james/go/bin/gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true,
        "hoverKind": "FullDocumentation",
        "analyses": {
          "unreachable": true,
          "unusedparams": true
        }
      }
    }
  },
  //Java
  //YAML
  "yaml.completion": true,
  "yaml.format.enable": true,
  "yaml.format.singleQuote": true,
  "yaml.hover": true,
  "yaml.schemas": {
    "kubernetes": ["/*.kub.yaml", "/*.kub.yml"],
    "https://json.schemastore.org/swagger-2.0": ["/*.swag.yaml", "/*.swag.yml"]
  }
}
